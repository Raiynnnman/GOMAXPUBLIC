
            select
                ic.id,
                ic.description,
                ic.code,
                iy.start_date > now(),
                iy.end_date > now()
            from
                icd_cpt ic,icd_year iy
            where
                ic.icd_year_id = iy.id and
                (description like '%Ambu%' or code like '%Ambu%') and
                iy.start_date > now() and iy.end_date > now()



            select
                ic.id,
                ic.description,
                ic.code,
iy.start_date > now()
            from
                icd_cpt ic,icd_year iy
            where
                ic.icd_year_id = iy.id and
                (description like '%Ambu%' or code like '%Ambu%') 
